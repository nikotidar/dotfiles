
#!/bin/sh
xinput --set-prop "TPPS/2 IBM TrackPoint" "Device Enabled" 0
xinput --set-prop "SynPS/2 Synaptics TouchPad" "Device Enabled" 1
xinput --set-prop "SynPS/2 Synaptics TouchPad" "libinput Tapping Enabled" 1
xinput --set-prop "SynPS/2 Synaptics TouchPad" "libinput Accel Speed" -0.60
#xinput --set-prop "Synaptics TM2749-001" "Device Enabled" 1
#xinput --set-prop "Synaptics TM2749-001" "libinput Tapping Enabled" 1
#xinput --set-prop "Synaptics TM2749-001" "libinput Accel Speed" -0.60
